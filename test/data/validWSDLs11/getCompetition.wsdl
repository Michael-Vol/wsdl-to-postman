                    <xsd:sequence>
                        <xsd:element name="application_key" type="xsd:string"/>
                        <xsd:element name="associationId" type="xsd:string" nillable="true"/>
                        <xsd:element name="teamSportId" type="xsd:string" nillable="true"/>
